#!/bin/bash
unset TOR_IP TORIP ; TOR_IP="$(proxychains -q curl 2ip.ru 2>/dev/null)" ; if [ ! -z "$TOR_IP" ] ; then TORIP=" | TOR IP : $TOR_IP" ; fi ; echo -e "PUB IP : $(curl ifconfig.io 2>/dev/null)$TORIP\nNET $(ip r|grep kernel|grep -o "dev .*proto"|awk '{print$2}') : $(ip r|grep -w "$(ip r|grep kernel|grep -o "dev .*proto"|awk '{print$2}')"|grep -om1 'src .*'|awk '{print$2}')"
